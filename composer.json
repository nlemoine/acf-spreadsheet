{
  "name": "hellonico/acf-color-palette",
  "description": "A Gutenberg like color palette field for ACF.",
  "type": "wordpress-plugin",
  "keywords": [
    "acf",
    "acf-field",
    "wordpress",
    "wordpress-plugin"
  ],
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "HelloNico\\AcfSpreadsheet\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "scripts": {
    "lint": [
      "phpcs --ignore=vendor --exclude=PSR1.Methods.CamelCapsMethodName --extensions=php --standard=PSR12 ."
    ]
  }
}
